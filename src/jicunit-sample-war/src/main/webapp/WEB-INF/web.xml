<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

  <display-name>JICUnit Sample Application</display-name>
  <description>
    This is a simple web application of how to use JICUnit
  </description>

  <context-param>
    <description>Enable Chuck Norris the programmer</description>
    <param-name>org.jicunit.framework.theme</param-name>
    <param-value>chucknorris</param-value>
  </context-param>


  <absolute-ordering>
    <name>jicunitfragment</name>
    <others />
  </absolute-ordering>

</web-app> 